{
  "name": "webpack-workspace",
  "version": "1.0.0",
  "description": "Webpack tempalte developed in v4",
  "keywords": [
    "webpack",
    "static",
    "starter",
    "html",
    "pages"
  ],
  "license": "MIT",
  "dependencies": {
    "inputmask": "^5.0.6",
    "normalize.css": "^8.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^7.3.1"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --mode development",
    "start:dist": "http-server dist --port ${PORT:-8080}",
    "build": "webpack --config webpack.prod.js --mode production",
    "preview": "npm run build && npm run start:prod",
    "test": "standard",
    "cy:run": "cypress run",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.11",
    "cypress": "^7.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.2",
    "http-server": "^0.12.3",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.1",
    "nunjucks-html-loader": "^1.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "4.2.0",
    "sass": "^1.43.4",
    "sass-loader": "10",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-glob-folder-entries": "^1.0.0"
  },
  "standard": {
    "ignore": [
      "cypress"
    ],
    "env": [
      "node",
      "es6"
    ]
  }
}
